<Window x:Class="examApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:examApp"
        mc:Ignorable="d"
        Title="{Binding ElementName=MainFrame, Path=Content.Title}" Height="450" Width="800"
        Icon="/Assets/1.png"
    >
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="60"/>
			<RowDefinition/>
		</Grid.RowDefinitions>
		<StackPanel Orientation="Horizontal" Grid.Row="0" >
			<Button x:Name="backBtn" Content="Nazad" Height="50" Width="100" Click="backBtnClick"/>
            <Button x:Name="authBtn" Content="Auth" Height="50" Width="100" Click="authBtnClick" HorizontalAlignment="Right"/>
        </StackPanel>
		<Frame x:Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Hidden"/>
	</Grid>
</Window>
